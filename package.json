{
  "name": "contentful-web",
  "version": "0.0.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Assembly-WebCrew/contentful-web.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build -e prod",
    "build:dev": "ng build -e srvdev -op ./dev",
    "build:stg": "ng build --prod -e stg -op ./stg",
    "build:prod": "ng build --prod -d code -op ./prod/code",
    "build:prod-aot": "ng build --prod -d code -op prod/code -bh .",
    "prod:copy": "cp prod/code/index.html prod/",
    "build:ci": "npm run build:dev && npm run build:stg && npm run build:prod && npm run prod:copy",
    "test": "ng test -cc --singleRun",
    "lint": "ng lint",
    "sass-lint": "sass-lint",
    "sass-lint-html": "sass-lint -v --format html -o sass-lint.html",
    "e2e": "ng e2e -e srvdev"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~6.1.0",
    "@angular/common": "~6.1.0",
    "@angular/compiler": "~6.1.0",
    "@angular/core": "~6.1.0",
    "@angular/forms": "~6.1.0",
    "@angular/http": "~6.1.0",
    "@angular/platform-browser": "~6.1.0",
    "@angular/platform-browser-dynamic": "~6.1.0",
    "@angular/router": "~6.1.0",
    "angulartics2": "^6.2.0",
    "apollo-cache-inmemory": "^1.3.0-beta.6",
    "apollo-client": "^2.3.7",
    "apollo-link-http": "^1.5.4",
    "core-js": "~2.5.5",
    "es6-shim": "^0.35.3",
    "graphql": "^0.13.2",
    "graphql-tag": "~2.9",
    "lodash": "^4.17.10",
    "normalize-scss": "~7.0.1",
    "qs": "~6.5.0",
    "rxjs": "~6.2.2",
    "rxjs-compat": "^6.0.0-rc.0",
    "showdown": "1.8.6",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.0",
    "@angular/cli": "^6.1.1",
    "@angular/compiler-cli": "~6.1.0",
    "@angular/language-service": "~6.1.0",
    "@types/jasmine": "~2.8.0",
    "@types/jasminewd2": "2.0.3",
    "@types/lodash": "~4.14.106",
    "@types/node": "~6.0.60",
    "@types/qs": "6.5.1",
    "codeclimate-test-reporter": "^0.5.0",
    "codelyzer": "~4.4.0",
    "husky": "^0.14.3",
    "jasmine-core": "3.1.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "^2.0.2",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "~1.2.0",
    "protractor": "~5.4.0",
    "sass-lint": "^1.12.1",
    "ts-node": "5.0.1",
    "tslint": "^5.11.0",
    "tslint-language-service": "0.9.9",
    "typescript": "~2.9.2"
  }
}
